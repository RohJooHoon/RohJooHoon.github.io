//튜토리얼
<style>
    .tutorialEdit {
        width: 338px;
        background: #000000;
        border-radius: 4px;
        display: flex;
        position: fixed;
        left: 50%;
        top: 50%;
        overflow: auto;
        transform: translate(-50%, -50%);
        flex-direction: column;
    }
    .tutorialEditBody {
        /*position: absolute;*/
    }
    .tutorialEditHeader {
        padding: 10px;
    }
    .tutorialEditHeaderTitle {
        font-weight: bold;
        font-size: 18px;
        line-height: 26px;
        color: #FFFFFF;
    }
    .tutorialEditHeaderTitle.is_sub {
        font-size: 12px;
        line-height: 17px;
    }
    .tutorialEditBodyInner {
        width: 317px;
        height: 317px;
        background-color: #fff;
        margin: 0 auto;
    }
    .tutorialEditBodyInnerText {
        font-weight: normal;
        font-size: 15px;
        line-height: 22px;
        color: #FFFFFF;
        padding: 8px 10px;
    }
    .tutorialEditBodyInnerText.is_sub {
        font-weight: 300;
        font-size: 12px;
        line-height: 17px;
        color: #FFFFFF;
    }
    .tutorialEditBtn {
        display: flex;
        flex-direction: row;
        justify-content: flex-end;
        padding: 0 10px 16px 0px;
    }
    .tutorialEditClose {
        width: 91px;
        height: 36px;
        background: #FFFFFF;
        border-radius: 2px;
        font-weight: 500;
        font-size: 14px;
        line-height: 20px;
        text-align: center;
    }

    /*default*/
    .tutorialDefault {
        width: 338px;
        background: #000000;
        border-radius: 4px;
        margin-top: 100px;
    }
    .tutorialDefaultHeader {
        padding: 16px 8px 10px;
    }
    .tutorialDefaultHeaderTitle {
        font-weight: 900;
        font-size: 18px;
        line-height: 26px;
        color: #FFFFFF;
    }
    .tutorialDefaultBodyInner {
        padding: 0px 10px;
    }
    .tutorialDefaultBodyInnerText {
        font-weight: normal;
        font-size: 15px;
        line-height: 22px;
        color: #FFFFFF;
    }
    .tutorialDefaultBodyInnerText.is_sub {
        font-weight: normal;
        font-size: 12px;
        line-height: 17px;
        color: #FFFFFF;
    }
    .tutorialDefaultClose {
        width: 91px;
        height: 36px;
        background: #FFFFFF;
        border-radius: 2px;
        margin-top: 16px;
    }
</style>
<script  id="TUTORIALEDIT" type="text/x-jsrender">
    <div class="popup tutorialEdit" style="z-index: {{:zIndex ? 1000 + zIndex : 1000}}; {{:maxWidth ? "max-width : " + maxWidth : ""}}>
        <div class="tutorialEditHeader">
            <div class="tutorialEditHeaderTitle is_sub">{{:subTitle}}</div>
            <strong class="tutorialEditHeaderTitle">{{:title}}</strong>
        </div>
        <div class="tutorialEditBody" style="display: block; margin-bottom: 16px">
            {{:html}}
        </div>
        <div class="tutorialEditBtn">
            <button class="tutorialEditClose" type="button" onClick="closePopup(this), nextTutorial(this);">{{:nextText ? nextText: "다음"}}</button>
        </div>
    </div>
</script>

<script  id="TUTORIALDEFAULT" type="text/x-jsrender">
    <div class="tutorial tutorialDefault" style="z-index: {{:zIndex ? 1000 + zIndex : 1000}}; {{:maxWidth ? "max-width : " + maxWidth : ""}}; {{:top ? "Top : " + Top : ""}}; {{:left ? "left : " + left : ""}}">
        <div class="tutorialDefaultHeader">
            <strong class="tutorialDefaultHeaderTitle">{{:title}}</strong>
        </div>
        <div class="tutorialDefaultBody" style="display: block;">
            {{:html}}
        </div>
        {{if next !== false}}
        <button class="tutorialDefaultClose" type="button" onClick="closePopup(this);{{:nextFunc}};">{{:nextText ? nextText: "시작하기"}}</button>
        {{/if}}
    </div>
</script>